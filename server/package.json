{
  "name": "server",
  "version": "0.0.0",
  "scripts": {
    "build": "npx ts-node build.ts",
    "lint": "npx eslint --ext .ts src/ spec/",
    "lint:fix": "npx eslint --fix --ext .ts src/ spec/",
    "lint:ci": "npx eslint --max-warnings=0 --output-file eslint_report.json --format json --ext .ts src/ spec/",
    "start": "node -r module-alias/register ./dist --env=production",
    "dev": "nodemon",
    "test": "nodemon --config ./spec/nodemon.json",
    "test:no-reloading": "npx ts-node --files -r tsconfig-paths/register ./spec"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@src": "dist"
  },
  "engines": {
    "node": ">=8.12.0"
  },
  "dependencies": {
    "@googleapis/youtube": "^8.0.2",
    "cookie-parser": "^1.4.6",
    "dedent": "^1.5.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.15.0",
    "fast-glob": "^3.3.1",
    "google-auth-library": "^8.7.0",
    "helmet": "^6.0.1",
    "inserturlparams": "^1.0.1",
    "jet-logger": "^1.3.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "node-fetch": "2",
    "sanitize-filename": "^1.6.3",
    "string-width": "4.1.0",
    "strip-ansi": "6.0.1",
    "ts-command-line-args": "^2.4.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jasmine": "^4.3.2",
    "@types/mock-fs": "^4.13.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "8.0.1",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "15.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "find": "^0.3.0",
    "fs-extra": "^11.1.0",
    "jasmine": "^5.0.0",
    "mock-fs": "^5.2.0",
    "nodemon": "^2.0.21",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "main": "index.js",
  "author": "gafirst",
  "license": "GPL-3.0"
}
