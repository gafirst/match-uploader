{
  "name": "server",
  "version": "0.0.0",
  "scripts": {
    "build": "npx ts-node build.ts prod",
    "build:sourcemaps": "npx ts-node build.ts",
    "lint": "npx eslint src/ spec/",
    "lint:fix": "npx eslint --fix  src/ spec/",
    "lint:ci": "npx eslint --max-warnings=0 --output-file eslint_report.json --format json src/ spec/",
    "start": "npx prisma migrate deploy && node -r module-alias/register ./dist --env=production",
    "start:worker": "node -r module-alias/register ./dist/worker.js --env=production",
    "dev": "nodemon",
    "dev:worker": "nodemon --exec \"./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src/worker.ts\"",
    "test": "nodemon --config ./spec/nodemon.json",
    "test:no-reloading": "npx ts-node --files -r tsconfig-paths/register ./spec"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@src": "dist"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@googleapis/youtube": "^8.0.2",
    "@numairawan/video-duration": "^1.0.0",
    "@prisma/client": "^6.0.0",
    "@sentry/node": "^10.25.0",
    "@sentry/profiling-node": "^10.25.0",
    "cookie-parser": "^1.4.7",
    "dedent": "^1.5.3",
    "dotenv": "^16.4.5",
    "express": "^5.0.1",
    "express-validator": "^7.2.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "google-auth-library": "^9.15.0",
    "graphile-worker": "^0.16.6",
    "helmet": "^8.0.0",
    "inserturlparams": "^1.0.1",
    "jet-logger": "^1.3.1",
    "luxon": "^3.5.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "mustache": "^4.2.0",
    "mv": "^2.1.1",
    "node-fetch": "2",
    "prisma": "^6.2.1",
    "sanitize-filename": "^1.6.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "string-width": "4.1.0",
    "strip-ansi": "6.0.1",
    "ts-command-line-args": "^2.5.1",
    "typo-js": "^1.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@graphile/logger": "^0.2.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.21",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jasmine": "^4.3.2",
    "@types/luxon": "^3.4.2",
    "@types/mock-fs": "^4.13.1",
    "@types/morgan": "^1.9.4",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.11.3",
    "@types/node-fetch": "^2.6.10",
    "@types/supertest": "^2.0.12",
    "@types/typo-js": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^9.16.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jasmine": "^4.2.2",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-n": "^17.14.0",
    "eslint-plugin-promise": "^7.2.1",
    "find": "^0.3.0",
    "jasmine": "^5.1.0",
    "mock-fs": "^5.2.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  },
  "resolutions": {
    "strip-ansi": "^6.0.1",
    "string-width": "^4.2.2",
    "wrap-ansi": "^7.0.0"
  },
  "main": "index.js",
  "author": "gafirst",
  "license": "GPL-3.0"
}
